{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "rootDir": "../.",
    "outDir": "../dist"
  },
  "exclude": ["node_modules"],
  "include": ["**/*.ts"]
}
